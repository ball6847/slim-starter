{
  "require": {
    "slim/slim": "^3.0@RC",
    "doctrine/dbal": "2.5.1",
    "doctrine/orm": "2.4.7",
    "doctrine/migrations": "dev-master",
    "mnapoli/php-di": "~4.0",
    "juliangut/slim-doctrine-middleware": "dev-master",
    "icanboogie/inflector": "^1.4"
  },
  "repositories" : [
    {
      "type":"package",
      "package": {
        "name": "juliangut/slim-doctrine-middleware",
        "version":"master",
        "source": {
          "url": "https://github.com/ball6847/slim-doctrine-middleware.git",
          "type": "git",
          "reference":"master"
        }
      }
    }
  ],
  "autoload": {
    "psr-4": {
      "Model\\": "models/",
      "Library\\": "library/",
      "Jgut\\Slim\\Middleware\\": "vendor/juliangut/slim-doctrine-middleware/src/"
    }
  }
}
